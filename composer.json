{
  "name": "basho/riak-pb",
  "description": "Official Riak Protocol Buffers client for PHP",
  "type": "library",
  "keywords": [
    "basho",
    "riak",
    "client",
    "driver",
    "database",
    "nosql",
    "kv",
    "datatype",
    "crdt",
    "data",
    "pb"
  ],
  "homepage": "http://basho.com/riak/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christopher Mancini",
      "email": "cmancini@basho.com",
      "homepage": "https://github.com/basho",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": {"Basho\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Basho\\Tests\\": "vendor/basho/riak/tests/"}
  },
  "support": {
    "issues": "https://github.com/basho/riak-phppb-client/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/basho/php-protobuf"
    }
  ],
  "require": {
    "basho/riak": "^3.3",
    "php": ">=5.5",
    "ext-json": "*",
    "ext-curl": "*",
    "google/protobuf": "v3.2.0-rc.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*",
    "apigen/apigen": "4.1.*"
  }
}
